<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>web.UI.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>web.UI.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DoRegister</servlet-name>
    <servlet-class>web.Controller.DoRegister</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DoLogin</servlet-name>
    <servlet-class>web.Controller.DoLogin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DoLogout</servlet-name>
    <servlet-class>web.Controller.DoLogout</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/servlet/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DoRegister</servlet-name>
    <url-pattern>/servlet/DoRegister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DoLogin</servlet-name>
    <url-pattern>/servlet/DoLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DoLogout</servlet-name>
    <url-pattern>/servlet/DoLogout</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
